{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/about/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/jobs/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/team/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/team-categories/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/blog/?$",
			"dest": "_render"
		},
		{
			"src": "^/career/?$",
			"dest": "_render"
		},
		{
			"src": "^/debug/?$",
			"dest": "_render"
		},
		{
			"src": "^/privacy-policy/?$",
			"dest": "_render"
		},
		{
			"src": "^/support/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		}
	]
}